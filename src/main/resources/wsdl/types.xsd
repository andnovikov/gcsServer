          <element name="extId" type="string"/>
          <element name="serviceId" type="string"/>
          <element name="outParams" type="tns:WsKeyPairDto" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  
  <!-- Types for {pmnt-check} -->
  <complexType name="WsPmntCheckDto">
